{
    "version": "0.1.0",
    "command": "go",
    "isShellCommand": true,
    "args": [
        "build"
    ],
    "showOutput": "always",
    "problemMatcher": {
        "owner": "go",
        "fileLocation": [
            "relative",
            "${workspaceRoot}"
        ],
        "pattern": {
            "regexp": "^\\.\\\\(.*)\\:([0-9]+)\\:\\s*(.*)$",
            "file": 1,
            "location": 2,
            "message": 3
        }
    }
}